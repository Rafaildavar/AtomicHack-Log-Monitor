{
  "buildCommand": "cd web && npm install && npm run build",
  "outputDirectory": "web/dist",
  "functions": {
    "vercel_api/index.py": {
      "runtime": "python3.11",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/.*",
      "dest": "vercel_api/index.py"
    },
    {
      "src": "/assets/.*",
      "dest": "web/dist/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "web/dist/$1"
    }
  ]
}
